<%- include('./header.ejs') %>
    <br />
    <br />
    <h1 class="text-center">Course display</h1>
    <div class="container">
        <table class="table table-bordered">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>City</th>
                <th>Status</th>
                <th>comment</th>
                <th>Action</th>
            </tr>
            <% d.forEach((item) =>{ %>
                <!-- console.log(item) -->
                <tr>
                    <td><%= item._id %></td>
                    <td><%= item.name %></td>
                    <td><%= item.email %></td>
                    <td><%= item.city %></td>
                    <td class="bg-warning"><%= item.status %></td>
                    <td><%= item.comment %></td>
                    <td>
                        <a href="/courseview/<%= item._id %>" class="btn btn-primary">View</a>
                        <a href="/courseEdit/<%= item._id %>" class="btn btn-info">Edit</a>
                    </td>
                </tr>
           <% })%>
            
        </table>
    </div>

<%- include('./footer.ejs') %>